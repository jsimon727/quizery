<button class="joinbutton">Join Quizery</button>
<form class="joinForm">
</form>
<div class="loginForm">
  <%= form_tag "/login" do %>
    <%= label_tag :email %>:
    <%= email_field_tag :email %></br>
    <%= label_tag :password %>:
    <%= password_field_tag :password %></br>
    
    <%= submit_tag "Log in!" %>
  <% end %>
</div>

<%= link_to("Teacher Dashboard", user_path(session[:user_id])) %>


<script>
  $('.login').click(function(){
    $('.loginForm').toggle("slow")
  })
</script>
